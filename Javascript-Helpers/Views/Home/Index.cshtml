@{
    ViewBag.Title = "Home Page";
}

<h1>String Functions</h1>
<div class="row">
    <div class="col-md-5">
        <h2>Content Manipulation</h2>
        <table class="table table-striped table-hover col-md-12">
            <tr>
                <th>String Function</th><th>Result</th>
            </tr>
            <tr>
                <td>
                    <details>
                        <summary class="h3">Name Extraction</summary>
                        <p>Acceptable formats:  Last, First Middle and First Middle Last</p>
                    </details>
                    <input type="text" id="string1" />
                    <fieldset id="string-format">
                        <label><input type="radio" value="lfm" name="string-format" />Last, First Middle</label>
                        <label><input type="radio" value="fml" name="string-format" />First Middle Last</label>
                    </fieldset>
                    <div id="string1-btn" onclick="test();" class="btn btn-success">Extract</div>
                </td>
                <td>
                    <div id="string1-result">Output</div>
                </td>
            </tr>
        </table>  
    </div>
</div>
